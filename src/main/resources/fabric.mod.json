{
  "schemaVersion": 1,
  "id": "viewboboptions",
  "version": "${version}",
  "name": "View Bobbing Options",
  "description": "View Bobbing Options as it should have been.",
  "authors": [ "GravityIO" ],
  "contact": {
    "sources": "https://github.com/GravityCY/ViewBobbingOptions",
    "issues": "https://github.com/GravityCY/ViewBobbingOptions"
  },
  "license": "MIT",
  "icon": "assets/viewboboptions/icon.png",
  "environment": "client",
  "entrypoints": {
    "preLaunch": [
      "me.gravityio.viewboboptions.ViewBobbingOptions"
    ],
    "client": [
      "me.gravityio.viewboboptions.ViewBobbingOptions"
    ],
    "modmenu": [
      "me.gravityio.viewboboptions.compat.ModMenuApiImpl"
    ]
  },
  "mixins": [
    "viewboboptions.mixins.json",
    "compat.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": ">=${fabric_version}",
    "minecraft": "~${minecraft_version}",
    "java": ">=17",
    "yet-another-config-lib": ">=1.7.1"
  },
  "recommends": {
    "modmenu": "*"
  }
}
